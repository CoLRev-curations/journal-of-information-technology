@article{HansethLyytinen2010,
   colrev_origin                 = {CROSSREF.bib/HansethLyytinen2010;
                                    DBLP.bib/000801;
                                    pdfs.bib/0000000178;},
   colrev_status                 = {pdf_prepared},
   colrev_masterdata_provenance  = {CURATED:https://github.com/CoLRev-curations/journal-of-information-technology;;},
   colrev_data_provenance        = {file:pdfs/25_1/Hanseth2010_Design Theory For .pdf;;
                                    abstract:http://dx.doi.org/10.1057/JIT.2009.19;;
                                    language:http://dx.doi.org/10.1057/JIT.2009.19;;
                                    cited_by:https://api.crossref.org/works/10.1057/JIT.2009.19;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|25|1|2010|hanseth-lyytinen|design-theory-for-dynamic-complexity-in-information-infrastructures-the-case-of-building-internet;},
   colrev_pdf_id                 = {cpid1:ffffffffffe00ffbffffffffc014100fc000000fc000125fc00fffffc03fffffc00ffbffc2000fffc0fffffff1ffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc0001fffcfffffffffffffffc011a62fc0000003c0000003c0000003c0000003c0000003c0010003ffffffffffffffff},
   file                          = {data/pdfs/25_1/HansethLyytinen2010.pdf},
   doi                           = {10.1057/JIT.2009.19},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/HansethL10},
   author                        = {Hanseth, Ole and Lyytinen, Kalle},
   journal                       = {Journal of Information Technology},
   title                         = {Design theory for dynamic complexity in information infrastructures - the case of building internet},
   year                          = {2010},
   volume                        = {25},
   number                        = {1},
   pages                         = {1--19},
   abstract                      = {We propose a design theory that tackles dynamic complexity in the design for Information Infrastructures (IIs) defined as a shared, open, heterogeneous and evolving socio-technical system of Information Technology (IT) capabilities. Examples of IIs include the Internet, or industry-wide Electronic Data Interchange (EDI) networks. IIs are recursively composed of other infrastructures, platforms, applications and IT capabilities and controlled by emergent, distributed and episodic forms of control. II's evolutionary dynamics are nonlinear, path dependent and influenced by network effects and unbounded user and designer learning. The proposed theory tackles tensions between two design problems related to the II design: (1) the bootstrap problem: IIs need to meet directly early users’ needs in order to be initiated; and (2) the adaptability problem: local designs need to recognize II's unbounded scale and functional uncertainty. We draw upon Complex Adaptive Systems theory to derive II design rules that address the bootstrap problem by generating early growth through simplicity and usefulness, and the adaptability problem by promoting modular and generative designs. We illustrate these principles by analyzing the history of Internet exegesis.},
   language                      = {eng},
   cited_by                      = {317},
}


   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|36|3|2021|alsaad-taamneh-sila-elrehail|understanding-the-global-diffusion-of-b2b-e-commerce-b2b-ec-an-integrated-model;},
   file                          = {data/pdfs/36_3/AlsaadTaamnehSilaEtAl2021.pdf},

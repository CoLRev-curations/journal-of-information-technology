   colrev_pdf_id                 = {cpid1:ffffffffffe01ffbffffffffc000403fc000017fc00103ffc007ffffc89fffffc003ffffc1eb9fffc01fffffe03fffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00403ffc00403ffc7ffffffc0000003c0010003c0008007c0010003c0000003c0000003c0000003ffffffffffffffff},

   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|23|1|2008|mahnke-wareham-bjorn-andersen|offshore-middlemen-transnational-intermediation-in-technology-sourcing;},
   colrev_pdf_id                 = {cpid1:ffffffffdff00fffffffffffc081007fc000003fc000001fc0001fffc000ffffc605ffffc1fa8fffc10fffffffffffffc7ffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00017ffc03fffffc7ffffffc0000003c0000003c0000003c0000003c0000003c0010003c0090003ffffffffffffffff},
   file                          = {data/pdfs/23_1/MahnkeWarehamBjornAndersen2008.pdf},
   doi                           = {10.1057/PALGRAVE.JIT.2000124},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/MahnkeWB08},
   author                        = {Mahnke, Volker and Wareham, Jonathan and Bjørn-Andersen, Niels},
   journal                       = {Journal of Information Technology},
   title                         = {Offshore middlemen - transnational intermediation in technology sourcing},
   year                          = {2008},
   volume                        = {23},
   number                        = {1},
   pages                         = {18--30},
   abstract                      = {The tendency of acquiring information systems and other high technology services from international suppliers continues at unprecedented levels. The primary motivation for the offshore sourcing of technology and services continues to be labor cost arbitrage, and secondly, access to higher levels of expertise. Yet paradoxically, large gaps in technical proficiency, cultural values, and communication styles between client and vendor can undermine the overall success of the offshore relationship. This paper argues that a new breed of entities have emerged, brokering or intermediating offshore relations. The capabilities of such ‘middlemen’ include moderating disparities in expertise, culture, and communication styles that often deteriorate performance in offshore relationships. The paper presents a preliminary theoretical justification for the emergence of offshore intermediaries, describes how and why they develop boundary spanning capabilities, and offers a case study as initial evidence substantiating the function and processes in intermediating transnational offshoring relationships. Our theory development concludes with propositions concerning four major offshore intermediary capabilities: (i) intermediating cultural distance, (ii) intermediating cognitive distance, (iii) pre-contractual preparation and negotiation, and (iv) post-contractual operational management.},
   language                      = {eng},
   cited_by                      = {53},
}


   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|32|1|2017|nicholson-babin-briggs|exploring-the-effects-of-liminality-on-corporate-social-responsibility-in-inter-firm-outsourcing-relationships;},
   colrev_pdf_id                 = {cpid1:ffffffffffe01ffbffffffffc00102ffc000004fc000064fc0003fffc223ffffc003ffffc7bfffffc00007ffffffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc0003fffc7ffffffc0000003c0000003c0018007c0008003c0018003c0010003c0018003c0010003c0018003c0018003ffffffffffffffff},

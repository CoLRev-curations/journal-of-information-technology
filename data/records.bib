@article{MarjanovicCecezKecmanovicVidgen2021,
   colrev_origin                 = {pdfs.bib/0000000118;},
   colrev_status                 = {md_prepared},
   colrev_masterdata_provenance  = {journal:https://api.crossref.org/works/10.1177/0268396221999671;;
                                    title:https://api.crossref.org/works/10.1177/0268396221999671;;
                                    volume:http://dx.doi.org/10.1177/02683962211010356;;
                                    number:http://dx.doi.org/10.1177/02683962211010356;;
                                    author:pdfs/36_4/Marjanovic.pdf;;
                                    year:https://api.crossref.org/works/10.1177/0268396221999671;;},
   colrev_data_provenance        = {file:pdfs/36_4/Marjanovic.pdf;;
                                    doi:pdfs/36_4/Marjanovic.pdf;;
                                    abstract:http://dx.doi.org/10.1177/02683962211010356;;
                                    language:http://dx.doi.org/10.1177/02683962211010356;;
                                    cited_by:https://api.crossref.org/works/10.1177/0268396221999671;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|36|4|2021|marjanovic-cecez-kecmanovic-vidgen|algorithmic-pollution-making-the-invisible-visible;},
   file                          = {data/pdfs/36_4/MarjanovicCecezKecmanovicVidgen2021.pdf},
   doi                           = {10.1177/02683962211010356},
   author                        = {Marjanovic, Olivera and Cecez-Kecmanovic, Dubravka and Vidgen, Richard},
   journal                       = {Journal of Information Technology},
   title                         = {Algorithmic pollution: Making the invisible visible},
   year                          = {2021},
   volume                        = {36},
   number                        = {4},
   abstract                      = {In this article, we focus on the growing evidence of unintended harmful societal effects of automated algorithmic decision-making in transformative services (e.g. social welfare, healthcare, education, policing and criminal justice), for individuals, communities and society at large. Drawing from the long-established research on social pollution, in particular its contemporary ‘pollution-as-harm’ notion, we put forward a claim – and provide evidence – that these harmful effects constitute a new type of digital social pollution, which we name ‘algorithmic pollution’. Words do matter, and by using the term ‘pollution’, not as a metaphor or an analogy, but as a transformative redefinition of the digital harm performed by automated algorithmic decision-making, we seek to make it visible and recognized. By adopting a critical performative perspective, we explain how the execution of automated algorithmic decision-making produces harm and thus performs algorithmic pollution. Recognition of the potential for unintended harmful effects of algorithmic pollution, and their examination as such, leads us to articulate the need for transformative actions to prevent, detect, redress, mitigate and educate about algorithmic harm. These actions, in turn, open up new research challenges for the information systems community.},
   language                      = {eng},
   cited_by                      = {2},
}

@article{OvrelidBygstad2019,
   colrev_origin                 = {pdfs.bib/0000000134;},

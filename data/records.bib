   colrev_pdf_id                 = {cpid1:ffffffffdff00fffefffffffc008020fc001c3bfc001ffffc11fffffc007ffffc00003ffc1ffffffffffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00003ffc1409fffffffffffc6dfbdffc0008003c0010003c0010003c0008003c0000003c0000003c0010003c0018003c0008003c0000003ffffffffffffffff},
   file                          = {data/pdfs/20_4/AllenKim2005.pdf},
   doi                           = {10.1057/PALGRAVE.JIT.2000048},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/AllenK05},
   author                        = {Allen, Jonathan P. and Kim, Jeffrey Y.},
   journal                       = {Journal of Information Technology},
   title                         = {IT and the video game industry - tensions and mutual shaping},
   year                          = {2005},
   volume                        = {20},
   number                        = {4},
   pages                         = {234--244},
   abstract                      = {This paper examines the influence of information technology (IT) on a distinct but closely related industry, the video game industry. We conceptualize the effects of IT as a process of translating three related dimensions of a technological frame - technology performance, industry practices, and use vision - from one industry to another. Through historical examples, we argue that the impact of IT on the video game industry is shaped and limited by this translation process, particularly when tensions between the two industries lead to the development of new complementary or replacement technologies, practices, or visions. Although heavily dependent on IT, the video game industry has had to ignore, postpone, or substantially modify important IT software tools, processors, storage media, graphics, and networking technologies because of these industry contradictions.},
   language                      = {eng},
   cited_by                      = {8},
}


   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|31|2|2016|choudrie-zamani|understanding-individual-user-resistance-and-workarounds-of-enterprise-social-networks-the-case-of-service-ltd;},
   colrev_pdf_id                 = {cpid1:ffffffffdff00fffffffffffc000200fc000001fc000003fc08001ffc20fffffc00fffffc20007ffc0ffffffffffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc0001fffffffffffc7ffffffc0000003c0010003c0010003c0010003c0010003c30d8003c0010003ffffffffffffffff},

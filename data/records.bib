   colrev_pdf_id                 = {cpid1:ffffffffffe00ffbffffffffc0208303c0000b8fc0583fffc1ffffffc000a3ffc07ffffff07fffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc0000bffc2041fffc7ffffffc0018003c0000003c0008007c0010003c0000003c0010003c0000003c0010003c0010003ffffffffffffffff},

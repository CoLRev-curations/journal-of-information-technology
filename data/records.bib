   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|16|2|2001|sorensen-lundh-snis|innovation-through-knowledge-codification;},
   colrev_pdf_id                 = {cpid1:fffffff3c000bff3c02987ffc07fffffc03fffffffffffffffffffffffffffffc000003fc000003fc000003fc000003fccb97fffffffffffc7ffffffc3ff0003c0018003c0018003c0018333c0018403c0018003c0018003c0018003c9008003c0018003c0018003c0018003c0018003c8018003cceb8003fff81ffffff01fff},

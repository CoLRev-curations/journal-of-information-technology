@article{BajwaLewisPervanEtAl2005,
   colrev_origin                 = {CROSSREF.bib/BajwaLewisPervanEtAl2005;
                                    DBLP.bib/000658;
                                    pdfs.bib/0000000352;},
   colrev_status                 = {pdf_prepared},
   colrev_masterdata_provenance  = {CURATED:https://github.com/CoLRev-curations/journal-of-information-technology;;},
   colrev_data_provenance        = {file:pdfs/20_2/Deepinder2005_The Adoption And.pdf;;
                                    abstract:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000037;;
                                    language:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000037;;
                                    cited_by:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000037;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|20|2|2005|bajwa-lewis-pervan-lai|the-adoption-and-use-of-collaboration-information-technologies-international-comparisons;},
   colrev_pdf_id                 = {cpid1:ffffffffdff00fffffffffffc022801fc000001fc000201fc01fffffc48001ffc0003fffc0003fffc0fffffff0ffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00003ffc007ffffffffffffc0018003c0000003c0009047c0008003c0000003ffffffffffffffff},
   file                          = {data/pdfs/20_2/BajwaLewisPervanEtAl2005.pdf},
   doi                           = {10.1057/PALGRAVE.JIT.2000037},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/BajwaLPL05},
   author                        = {Bajwa, Deepinder S. and Lewis, L. Floyd and Pervan, Graham and Lai, Vincent S.},
   journal                       = {Journal of Information Technology},
   title                         = {The adoption and use of collaboration information technologies - international comparisons},
   year                          = {2005},
   volume                        = {20},
   number                        = {2},
   pages                         = {130--140},
   abstract                      = {Information technology (IT) applications to support group decision processes have been of considerable interest over the years. With the emergence of virtual team arrangements and the advent of emerging information and networking technologies, an increasing amount of attention is now being aimed at understanding collaboration among group members, as they make decisions to accomplish tasks. Effective and efficient collaboration is critical from a decision quality and decision timeliness standpoint. Commonly known as collaborative information technologies (CITs), many technology solutions have the capability to enhance collaboration and facilitate group decisions in task accomplishment by enabling better communication, sharing of information, ideas, expertise, and evaluating alternatives, irrespective of time and distance barriers. Many studies have investigated such individual CIT solutions in different regional settings. However, despite the fact that no single medium can support collaboration in different types of tasks, there is a scarcity of research investigating the adoption and use of multiple CIT options across regions. This paper builds upon innovation diffusion theory and tests a research model to validate five antecedents of collective adoption and use of seven CITs in the US, Australia, and Hong Kong. Sub-sample analyses of data collected from 344 organizations in these three regions suggest that not only do adoption and use patterns of some CIT solutions vary across regions but so do the antecedents that explain their proliferation. Implications of our findings are discussed for practitioners and researchers.},
   language                      = {eng},
   cited_by                      = {40},
}


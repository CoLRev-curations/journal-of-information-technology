   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|20|1|2005|swanson-wang|knowing-why-and-how-to-innovate-with-packaged-business-software;},
   colrev_pdf_id                 = {cpid1:ffffffffdff00fffffffffffc010700fc00006dfc00007ffc01fffffc00367ffc0ffffffc07fffffefffffffc20a03ffc00001ffc00001ffc00001ffc00001ffc02733ffc00603ffefffffffffffffffc0000003c0008003c0010003c0000003c0010003c0000003c0010003c0010003c0000003c0018003ffffffffffffffff},
   file                          = {data/pdfs/20_1/SwansonWang2005.pdf},
   doi                           = {10.1057/PALGRAVE.JIT.2000033},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/SwansonW05},
   author                        = {Swanson, E. Burton and Wang, Ping},
   journal                       = {Journal of Information Technology},
   title                         = {Knowing why and how to innovate with packaged business software},
   year                          = {2005},
   volume                        = {20},
   number                        = {1},
   pages                         = {20--31},
   abstract                      = {When firms move to adopt and implement a popular IT innovation, what knowledge must they have or gain, in order to be successful? Here we offer a model that explains a firm's success in terms of its adoption know-why and know-when and its implementation know-how. We examine this model in an exploratory survey of some 118 firms’ adoption and implementation of packaged business software in the 1990s. Using multivariate methods, we identify business coordination as know-why and management understanding and vendor support as know-how factors important to success, explaining nearly 60% of the variance. There is limited evidence that the right adoption know-why may help in acquiring or fostering the right implementation know-how. The findings serve to remind practitioners that they should attend carefully to adoption rationales, grounded in business benefits, when they innovate with new IT.},
   language                      = {eng},
   cited_by                      = {22},

   colrev_pdf_id                 = {cpid1:ffffffffdff00fffffffffffc080400fc006660fc007ffffc01fffffc0007fffc24fffffc0400fffffffffffc41807ffc00001ffc00001ffc00001ffc00001ffc00001ffc0001fffc1ffffffffffffffc7ffffffc0010003c0010003c0010003c0008007c0008003c0010003c0110003c0000003c0000003ffffffffffffffff},

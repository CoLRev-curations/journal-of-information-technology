   colrev_pdf_id                 = {cpid1:ffffffffffe00ffbffffffffc10081ffc00001ffc00007ffc0001fffc03fffffc007ffffc007dbffc1ffffffc3ffffffefffffffc00001ffc00001ffc00001ffc00001ffc00001ffc0000fffc0000fffceffffffc7ffffffe0000003e0000003e7ff0003dfff0003c0008207c0000003c0000003c0000003ffffffffffffffff},
   file                          = {data/pdfs/23_4/FinkMarkovich2008.pdf},
   doi                           = {10.1057/JIT.2008.14},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/FinkM08},
   author                        = {Fink, Lior and Markovich, Sarit},
   journal                       = {Journal of Information Technology},
   title                         = {Generic verticalization strategies in enterprise system markets - An exploratory framework},
   year                          = {2008},
   volume                        = {23},
   number                        = {4},
   pages                         = {281--296},
   abstract                      = {In recent years, enterprise system (ES) software markets have been very dynamic. While contemporary customers are increasingly seeking ES solutions that require less and less customization and implementation effort, it is unclear whether all ES providers should take the ‘vertical’ path of offering functionality tailored to specific industries. Given the lack of conceptualization that explores ES markets’ segmentation, this paper offers a typology of generic verticalization strategies. Building on the resource-based view of the firm and the dynamic capabilities perspective, we match ES providers’ organizational characteristics of size and scope with the most effective verticalization strategy. A dynamic dimension is introduced to this framework by analyzing recommended strategies for market entry and growth. Finally, the applicability of the exploratory framework is illustrated using examples from the customer relationship management (CRM) software market.},
   language                      = {eng},
   cited_by                      = {9},
}


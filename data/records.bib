   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|17|4|2002|brooke|critical-perspectives-on-information-systems-an-impression-of-the-research-landscape;},
   colrev_pdf_id                 = {cpid1:ffffffffdbafffe3c00003ffc0000fffc07fffffffffffffffffffffc9b7cdbfc000001fc000001fc000001fc000001fc000001fffffffffffffffffc0078003c0078003e0018003e00196bbc01fffffc00081ffc0008003c0008003c1258003c0018003c0008003c0008003c0018003c0018003fffdfefffff81ffffff00fff},

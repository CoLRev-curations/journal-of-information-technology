   colrev_pdf_id                 = {cpid1:ffffffffdff00fffefffffffc00000ffc07200ffc0ffffffc7ffffffc01fffffc1ffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00013ffc05dffffcfffffffc0010003c0000003c0010003c001ffffc00003ffc0000003c0010003c0000003c0000003c0000003c0000003ffffffffffffffff},

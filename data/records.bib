   colrev_pdf_id                 = {cpid1:ffffffffffe00ffbffffffffc0000007c0e7e7dfc0ffffffc0003fffc000003fc3fffbffc0006bffffffffffc06143ffc00001ffc00001ffc00001ffc00001ffc00003ffc0001fffc03fffffffffffffc7ff8203c0010003c0008003c0018003c0010003c0010003c0018003c0010003c0010003c0008003ffffffffffffffff},

   colrev_pdf_id                 = {cpid1:ffffffffffe00ffbffffffffc0084007c000001fc050029fc3ffffffc1ffffffc7afffffc0000fffc0ffffffffffffffcbffffffc00001ffc00001ffc00001ffc00001ffc0001fffc0001fffefffffffc3379aefc0000003c0000003c0000003c0010003c0000003c000ffffc0000037c0000003c0010003ffffffffffffffff},
   file                          = {data/pdfs/27_2/Walsham2012a.pdf},
   doi                           = {10.1057/JIT.2012.4},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/Walsham12},
   author                        = {Walsham, Geoff},
   journal                       = {Journal of Information Technology},
   title                         = {Are we making a better world with ICTs? Reflections on a future agenda for the IS field},
   year                          = {2012},
   volume                        = {27},
   number                        = {2},
   pages                         = {87--93},
   abstract                      = {Dramatic changes in context have occurred over the last 25 years for those of us working in the information systems (IS) field, with ICTs becoming pervasive in many areas of human activity and in all countries of the world. It is ironic that the success of ICTs has led to concerns that the distinct research contribution of the IS field could become lost and that the field itself may decline. The literature over the last decade or so has offered different and sometimes opposing suggestions as to how the field should respond to these concerns. In this paper, I discuss these debates and I use the discussion as a basis for putting forward a skeleton future agenda for the IS field as a whole. This agenda emphasises the need for a focus on ethical goals, increased use of critical approaches, welcoming other disciplines with open arms, widening our field of study to many nontraditional settings and rejecting a dominant methodological paradigm.},
   language                      = {eng},
   cited_by                      = {154},
}


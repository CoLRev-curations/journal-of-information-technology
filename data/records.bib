   colrev_masterdata_provenance  = {journal:http://dx.doi.org/10.1177/0268396219834122;;
                                    title:http://dx.doi.org/10.1177/0268396219834122;;
                                    volume:http://dx.doi.org/10.1177/0268396219834122;;
                                    number:http://dx.doi.org/10.1177/0268396219834122;;
                                    author:pdfs/34_2/grover2019.pdf;;
                                    year:LOADER.import_provenance;;},
   colrev_data_provenance        = {file:pdfs/34_2/grover2019.pdf;;
                                    doi:pdfs/34_2/grover2019.pdf;;
                                    abstract:http://dx.doi.org/10.1177/0268396219834122;;
                                    language:http://dx.doi.org/10.1177/0268396219834122;;
                                    cited_by:http://dx.doi.org/10.1177/0268396219834122;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|34|2|2019|grover-carter-jiang|trends-in-the-conduct-of-information-systems-research;},
   file                          = {data/pdfs/34_2/GroverCarterJiang2019a.pdf},
   doi                           = {10.1177/0268396219834122},
   author                        = {Grover, Varun and Carter, Michelle and Jiang, Dan},
   journal                       = {Journal of Information Technology},
   title                         = {Trends in the conduct of information systems research},
   year                          = {2019},
   volume                        = {34},
   number                        = {2},
   abstract                      = {With the rapidly changing institutional and technological environment for information systems research, it is useful to examine how the conduct of research itself has been changing. In this Debates and Perspectives article, based on analysis of over 1800 articles, we first describe trends from 2000 to 2015 in co-authorship, research areas, unit of analysis, and research direction-setting initiatives. Our results indicate a rising trend among information systems researchers toward co-authorship. We also find that information systems research converges on a few broad research areas, and there is increasing diversity of topics within these broad themes. Concerning level of analysis, our results indicate a decreasing trend toward macro (organizational) studies. Finally, we note a lack of positive significant trends in the publication of special issue articles, review articles, and meta-analytic studies. We then offer our perspective regarding these trends along with a discussion of the importance of managing vicious and virtuous cycles that may result from community adaptations to environmental changes.},
   language                      = {eng},
   cited_by                      = {2},
}


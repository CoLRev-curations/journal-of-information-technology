   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|17|1|2002|corrocher-ordanini|measuring-the-digital-divide-a-framework-for-the-analysis-of-cross-country-differences;},
   colrev_pdf_id                 = {cpid1:ffffffe3c003ffe3c00007ffc00c87ffc01fffffc07fffffffffffffffffffffffffffffc000001fc000003fc000003fc000003feff7deffffffffffc7ffa463c6b10003c0018003c0018003c00184ebc0018003c001800bc0018003c0018003c0018003c0018003c0018003c0018003c0018003ddfdfffffff81ffffff01fff},
   file                          = {data/pdfs/17_1/CorrocherOrdanini2002.pdf},

   colrev_data_provenance        = {file:pdfs/22_2/Kotlarsky2007_Globally Distrib.pdf;;
                                    abstract:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000084;;
                                    language:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000084;;
                                    cited_by:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000084;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|22|2|2007|kotlarsky-oshri-van-hillegersberg-kumar|globally-distributed-component-based-software-development-an-exploratory-study-of-knowledge-management-and-work-division;},
   colrev_pdf_id                 = {cpid1:ffffffffffe007fbffffffffc100001fc000001fc000001fc001001fc03fffffc22003ffc000ffffc000ffffc00007ffc0ffffffffffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00303ffc00403ffefffffffc5f7bedfc0000003c0008003c0000003ffffffffffffffff},
   file                          = {data/pdfs/22_2/KotlarskyOshrivanHillegersbergEtAl2007.pdf},
   doi                           = {10.1057/PALGRAVE.JIT.2000084},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/KotlarskyOHK07},
   author                        = {Kotlarsky, Julia and Oshri, Ilan and van Hillegersberg, Jos and Kumar, Kuldeep},
   journal                       = {Journal of Information Technology},
   title                         = {Globally distributed component-based software development - an exploratory study of knowledge management and work division},
   year                          = {2007},
   volume                        = {22},
   number                        = {2},
   pages                         = {161--173},
   abstract                      = {Component-based development (CBD) can be an appealing proposition to globally distributed software development teams because of the almost endless possibilities to recombine and reuse components in new products. In particular, it has been suggested that CBD will improve globally distributed software development practices by allowing each site to take ownership of particular components, resulting in reduced inter-site communication and coordination activities. Such an approach may indeed overcome breakdowns in inter-site coordination efforts; however, it may also lessen opportunities to share knowledge between sites and may hamper opportunities to reuse existing components. A case study approach, exploratory in nature, was adopted to explore knowledge aspects in global component-based software development. Evidence collected at several globally distributed CBD projects suggests that the true potential of CBD, which mainly relates to reusing components, can also be achieved when components are developed in a joint manner (i.e. by several sites) by accessing and utilizing expertise regardless of its geographical location. To improve the rate of component reuse, the studied teams developed capabilities in three particular areas: inter-site coordination, communications, and knowledge management. The paper concludes by discussing the links between component reuse, CBD principles and organizational capabilities, and offers managers and engineers some guidelines to consider in their CBD projects.},
   language                      = {eng},
   cited_by                      = {52},
}


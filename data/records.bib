   doi                           = {10.1057/PALGRAVE.JIT.2000014},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/Schneberger04},
   author                        = {Schneberger, Scott L.},
   journal                       = {Journal of Information Technology},
   title                         = {STATER NV - e-Servicing strategies},
   year                          = {2004},
   volume                        = {19},
   number                        = {2},
   pages                         = {108--116},
   abstract                      = {After 2 years of on-line experiments, Tom van Vianen, CEO, felt certain it was time to fully implement STATER NV's new ‘e-servicing’ concept with a cohesive strategy. Established in 1997 in The Netherlands and headquartered in Amersfoort, STATER had 27 business clients and serviced over 80 different mortgage portfolios of more than 450,000 mainly residential loans in the Netherlands, Belgium, and Germany. Their mortgage service operations and information systems were considered state-of-the-art in 2002, but they were moving business on-line while simultaneously increasing the types of services provided and expanding operations into Spain, France, and Italy — within the next five years. In a land known for taming the forces of the sea, Tom faced what seemed like a sea of ‘e-uncertainty.’ What exact roles should STATER play in an on-line loan market? How should they position themselves to lead in those roles? The E-Servicing Steering Committee looked to Tom to direct them, and he knew he needed a clear vision for the next Steering Committee meeting in 2 months, in May 2002.},
   language                      = {eng},
   cited_by                      = {0},
}

@article{SchneiderSunyaev2016,
   colrev_origin                 = {CROSSREF.bib/SchneiderSunyaev2016;
                                    DBLP.bib/001015;
                                    pdfs.bib/0000000360;},
   colrev_status                 = {pdf_prepared},
   colrev_masterdata_provenance  = {CURATED:https://github.com/CoLRev-curations/journal-of-information-technology;;},
   colrev_data_provenance        = {file:pdfs/31_1/schneider2014.pdf;;
                                    abstract:http://dx.doi.org/10.1057/JIT.2014.25;;
                                    language:http://dx.doi.org/10.1057/JIT.2014.25;;
                                    cited_by:http://dx.doi.org/10.1057/JIT.2014.25;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|31|1|2016|schneider-sunyaev|determinant-factors-of-cloud-sourcing-decisions-reflecting-on-the-it-outsourcing-literature-in-the-era-of-cloud-computing;},
   colrev_pdf_id                 = {cpid1:ffffffffffe01ffbffffffffc000003fc000000fc0000007c000001fc01fffffc4ffffffc001ffffc0ffffffffffffffcfffffffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00001ffc00000ffcbffffffc7ffffffc001817fc0008003c0010003c0010003c0000003c0010003c0008003ffffffffffffffff},
   file                          = {data/pdfs/31_1/SchneiderSunyaev2016.pdf},
   doi                           = {10.1057/JIT.2014.25},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/SchneiderS16},
   author                        = {Schneider, Stephan and Sunyaev, Ali},
   journal                       = {Journal of Information Technology},
   title                         = {Determinant factors of cloud-sourcing decisions - reflecting on the IT outsourcing literature in the era of cloud computing},
   year                          = {2016},
   volume                        = {31},
   number                        = {1},
   pages                         = {1--31},
   abstract                      = {Cloud computing (CC) is an emerging form of IT outsourcing (ITO) that requires organizations to adjust their sourcing processes. Although ITO researchers have established an extensive knowledge base on the determinant factors that drive sourcing decisions from various theoretical perspectives, the majority of research on cloud-sourcing decisions focuses on technological aspects. We reviewed the CC and ITO literature and systematically coded the determinant factors that influence sourcing decisions. We show that most determinant factors of sourcing decisions in the ITO context remain valid for the CC context. However, the findings for some factors (i.e., asset specificity, client firm IT capabilities, client firm size, institutional influences, and uncertainty) are inconclusive for the CC and ITO contexts. We discuss how the peculiarities of CC can explain these inconclusive findings. Our results indicate that CC researchers should draw from research on ITO decision making but re-examine ITO concepts in the light of the peculiarities of CC, such as the differences between software and infrastructure services, the self-service procurement of cloud services, or the evolving role of IT departments. By summarizing determinant factors of cloud-sourcing decisions for consideration in future research, we contribute to the development of endogenous theories in the IS domain.},
   language                      = {eng},
   cited_by                      = {117},
}


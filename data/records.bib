   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|18|2|2003|sabherwal-newman|persistence-and-change-in-system-development-a-dialectical-view;},
   colrev_pdf_id                 = {cpid1:ffffffffffffffffe0003fc7e00003ffe07fffffe03fffffe07fffffffffffffffffffffe000007fe000007fe000007fe000007fe01fffffffffffffffffffffe1ff8007e0008007e0018007e0008007e0008007e0008007e0008007e0008007e0008007e0008007e0018007e0298007e0008007fffbbffffff81fffffffffff},
   file                          = {data/pdfs/18_2/SabherwalNewman2003.pdf},
   doi                           = {10.1080/0268396032000101144},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/SabherwalN03},
   author                        = {Sabherwal, Rajiv and Newman, Michael},
   journal                       = {Journal of Information Technology},
   title                         = {Persistence and change in system development - a dialectical view},
   year                          = {2003},
   volume                        = {18},
   number                        = {2},
   pages                         = {69--92},
   abstract                      = {This paper seeks to advance our understanding of the dynamics of persistence and change in information systems development. Towards this goal, it draws upon dialectical theory, which enables viewing persistence and change during IS development relative to the thesis operating at that time. This approach recognizes that the nature of the system itself often changes during the development process. This also allows simultaneous attention to persistence and change, combining persistence with certain elements of the thesis along with change in some other elements. Empirically based on three cases, the paper provides insights into the content and process elements of theses, governing the characteristics of the system and the nature of the development process, respectively. It offers one possible explanation for the evolution of the IS development process and some insights into the strategies for improving this process through greater attention to the theses and the dialectic processes.},
   language                      = {eng},
   cited_by                      = {23},
}


   colrev_data_provenance        = {file:pdfs/23_2/Deborah2008_An Exploration Of .pdf;;
                                    abstract:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000134;;
                                    language:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000134;;
                                    cited_by:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000134;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|23|2|2008|bunker-kautz-anhtuan|an-exploration-of-information-systems-adoption-tools-and-skills-as-cultural-artefacts-the-case-of-a-management-information-system;},
   colrev_pdf_id                 = {cpid1:ffffffffffe00ffbffffffffc010001fc000007fc080007fc080001fc003ffffc1087fffc0087fffc75fffffc14001ffc1ffffffffffffffcfffffffc00001ffc00001ffc00001ffc00101ffc00003ffefffffffc7ffffffc0000003c0008023c0008003c0010003c0018003c0010003c0000003c0008003ffffffffffffffff},
   file                          = {data/pdfs/23_2/BunkerKautzAnhtuan2008.pdf},
   doi                           = {10.1057/PALGRAVE.JIT.2000134},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/BunkerKA08},
   author                        = {Bunker, Deborah and Kautz, Karlheinz and Anhtuan, Anhtai},
   journal                       = {Journal of Information Technology},
   title                         = {An exploration of information systems adoption - tools and skills as cultural artefacts - the case of a management information system},
   year                          = {2008},
   volume                        = {23},
   number                        = {2},
   pages                         = {71--78},
   abstract                      = {This paper explains the development of a skills-focused approach that can assist organisations to better anticipate hurdles to successful information systems (IS) adoption. This approach is utilised in an interpretive field study in an Australian information technology company. From a perspective that views IS as tools, the approach is used to analyse the management control skills required to use a specific management IS. A skills match between the set of management control skills assumed by the toolmaker and the skills possessed by the tool user shows why a group of users with a high degree of match adopted the tool, while another one with a low degree of match did not do so. The study thus demonstrates that the skills-focused approach is a valid and effective way of determining the appropriateness of an IS.},
   language                      = {eng},
   cited_by                      = {9},
}

@article{BunkerKautzNguyen2007,
   colrev_origin                 = {CROSSREF.bib/BunkerKautzNguyen2007;
                                    DBLP.bib/000721;
                                    pdfs.bib/0000000404;},
   colrev_status                 = {pdf_prepared},
   colrev_masterdata_provenance  = {CURATED:https://github.com/CoLRev-curations/journal-of-information-technology;;},
   colrev_data_provenance        = {file:pdfs/22_1/Deborah2007_Role Of Value Comp.pdf;;
                                    abstract:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000092;;
                                    language:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000092;;
                                    cited_by:http://dx.doi.org/10.1057/PALGRAVE.JIT.2000092;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|22|1|2007|bunker-kautz-nguyen|role-of-value-compatibility-in-it-adoption;},
   colrev_pdf_id                 = {cpid1:ffffffffffe00ffbffffffffc110000fe0001e1fc0001fffc00001ffc1ffffffe1ffffffcfffffffc00003ffc00001ffc00001ffc00001ffc00001ffc00001ffc00003ffc00fffffffffffffc7fb9df7c0010003c0010003c0018003c0008003c0050003c0010003c0010003c0010003c0010003c0010003ffffffffffffffff},
   file                          = {data/pdfs/22_1/BunkerKautzNguyen2007.pdf},
   doi                           = {10.1057/PALGRAVE.JIT.2000092},
   dblp_key                      = {https://dblp.org/rec/journals/jitech/BunkerKN07},
   author                        = {Bunker, Deborah and Kautz, Karlheinz and Nguyen, Anne Luu Thanh},
   journal                       = {Journal of Information Technology},
   title                         = {Role of value compatibility in IT adoption},
   year                          = {2007},
   volume                        = {22},
   number                        = {1},
   pages                         = {69--78},
   abstract                      = {Compatibility has been recognised as an important element in the adoption of IT innovations in organisations but as a concept it has been generally limited to technical or functional factors. Compatibility is also significant, however, with regard to value compatibility between the organisation, and the adopted IT innovation. We propose a framework to determine value compatibility analysing the organisation's and information system's structure, practices and culture, and explore the value compatibility of an organisation with its adopted self-service computer-based information system. A case study was conducted to determine the congruence of an organisation's value and IT value compatibility. This study found that there was a high correspondence in the organisational structure and practice dimensions; however, there were organisational culture disparities. The cultural disparities reflected the self-service acceptance and training issues experienced by the case organisation. These findings add insight into the problems experienced with value compatibility and the adoption of the information systems, and show the potential use of the proposed framework in the detection of such problems.},
   language                      = {eng},
   cited_by                      = {32},
}


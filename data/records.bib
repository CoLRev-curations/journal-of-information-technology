                                    cited_by:https://api.crossref.org/works/10.1177/0268396220912745;;},
   colrev_id                     = {colrev_id1:|a|journal-of-information-technology|35|3|2020|moeini-simeonova-galliers-wilson|theory-borrowing-in-it-rich-contexts-lessons-from-is-strategy-research;},
   file                          = {data/pdfs/35_3/MoeiniSimeonovaGalliersEtAl2020.pdf},
   doi                           = {10.1177/0268396220912745},
   author                        = {Moeini, Mohammad and Simeonova, Boyka and Galliers, Robert D and Wilson, Alex},
   journal                       = {Journal of Information Technology},
   title                         = {Theory borrowing in IT-rich contexts: Lessons from IS strategy research},
   year                          = {2020},
   volume                        = {35},
   number                        = {3},
   abstract                      = {While indigenous theorizing in information systems has clear merits, theory borrowing will not, and should not, be eschewed given its appeal and usefulness. In this article, we aim at increasing our understanding of modifying of borrowed theories in IT-rich contexts. We present a framework in which we discuss how two recontextualization approaches of specification and distinction help with increasing the IT-richness of borrowed constructs and relationships. In doing so, we use several illustrative examples from information systems strategy. The framework can be used by researchers as a tool to explore the multitude of ways in which a theory from another discipline can yield the understanding of IT phenomena.},
   language                      = {eng},
   cited_by                      = {5},
}

@article{SchreieckWiescheKrcmar2021,
